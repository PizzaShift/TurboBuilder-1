{

    "$schema": "https://raw.githubusercontent.com/edertone/TurboBuilder/master/TurboBuilder-Node/src/main/resources/json-schema/turbodepot.schema.json",
    
    "sources": {
    
        "fileSystem": [
            {
                "name": "source1",
                "path": "somepath"
            }
        ],
        
        "mysql": [
            {
                "name": "source2",
                "host": "db_host",
                "database": "db_name"
            }
        ],
        
        "sqLite": [
            {
                "name": "source3",
                "path": "somepath"
            }
        ]   
    },
    
    "depots": [
        
        {
            "name": "depot1",
            "files": {
                "source": "source1"
            },
            "objects": {
                "source": "source1"
            },
            "users": {
                "source": "source1",
                "tokenLength": 175,
                "tokenLifetime": 60
            }
        }   
    ]
}