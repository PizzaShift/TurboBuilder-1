{

    "$schema": "https://raw.githubusercontent.com/edertone/TurboBuilder/master/TurboBuilder-Node/src/main/resources/json-schema/turbodepot.schema.json",
    
    "sources": {
    
        "fileSystem": [
            {
                "name": "fs_source_1",
                "path": "somepath"
            }
        ],
        
        "mysql": [
            {
                "name": "mysql_source_1",
                "host": "db_host",
                "database": "db_name",
                "prefix": "tdp_"
            }
        ],
        
        "sqLite": [
            {
                "name": "sqlite_source_1",
                "path": "somepath",
                "prefix": "tdp_"
            }
        ]   
    },
    
    "depots": [
        
        {
            "name": "depot1",
            "files": {
                "source": "fs_source_1"
            },
            "logs": {
                "source": "fs_source_1"
            },
            "objects": {
                "source": "fs_source_1"
            },
            "users": {
                "source": "fs_source_1",
                "tokenLength": 175,
                "tokenLifetime": 60
            }
        }   
    ]
}